        </activity>
        <activity android:name="com.ccflying.glestriangle.TriangleActivity"></activity>
        <activity android:name="com.ccflying.glescircle.CircleActivity"></activity>
        <activity android:name="com.ccflying.gles3dimensional.ThreeDActivity"></activity>
    </application>

</manifest>